{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "anhskohbo/no-captcha": "^3.1",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "yajra/laravel-datatables-oracle": "^9.21",
        "yajra/laravel-oci8": "^6.1",
        "yajra/laravel-pdo-via-oci8": "^2.2"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "larastan/larastan": "1.0.3",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "quality:static": [
            "vendor/bin/phpstan analyse --configuration=phpstan.neon.dist --memory-limit=1G"
        ],
        "quality:style": [
            "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --dry-run --diff --using-cache=no"
        ],
        "quality:test:critical": [
            "vendor/bin/phpunit tests/Feature --filter '(DeleteRouteProtectionTest|SecuritySqlInjectionRegressionTest|SecurityUploadHardeningTest|TransactionBoundaryRollbackTest|AuthStateConsistencyTest|PriorityOneMaintainabilityFlowsTest|PriorityTwoCriticalPathTransitionsTest)'"
        ],
        "security:guardrail": [
            "@php tools/security_guardrail_check.php"
        ]
    }
}
